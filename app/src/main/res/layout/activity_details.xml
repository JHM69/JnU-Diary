<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:card_view="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".Activity.AddEntryActivity">    <androidx.appcompat.widget.Toolbar        android:id="@+id/toolbars"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="@color/colorAccent"        android:minHeight="?attr/actionBarSize"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">    </androidx.appcompat.widget.Toolbar>    <TextView        android:id="@+id/namet"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:fontFamily="@font/hind_siliguri_bold"        android:text="Jahangir Hossain"        android:textColor="@color/dim"        android:textIsSelectable="true"        android:textSize="24sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/toolbars" />    <TextView        android:id="@+id/ast"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:fontFamily="@font/hind_siliguri_medium_regular"        android:text="Chairman"        android:textColor="@color/dim"        android:textSize="18sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/namet" />    <TextView        android:id="@+id/ast2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="232dp"        android:fontFamily="@font/hind_siliguri_medium_regular"        android:text="নম্বরঃ 01635191148"        android:textColor="@color/dim"        android:textSize="18sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.188"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/namet" />    <TextView        android:id="@+id/ast3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:fontFamily="@font/hind_siliguri_medium_regular"        android:text="নম্বরঃ 01635191148"        android:textColor="@color/dim"        android:textSize="18sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.942"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/namet"        card_view:layout_constraintStart_toEndOf="@+id/ast2"        card_view:layout_constraintTop_toTopOf="@+id/ast2" />    <TextView        android:id="@+id/mailt"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:fontFamily="@font/hind_siliguri_medium_regular"        android:gravity="center_vertical"        android:singleLine="true"        android:text="CSE"        android:textColor="@color/dim"        android:textSize="18sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/ast" />    <androidx.cardview.widget.CardView        android:id="@+id/call"        android:layout_width="45dp"        android:layout_height="30dp"        android:clickable="true"        android:foreground="?android:attr/selectableItemBackground"        android:orientation="vertical"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/mailt"        card_view:cardBackgroundColor="@color/colorPrimaryDark"        card_view:cardCornerRadius="20dp"        card_view:cardElevation="7dp"        card_view:layout_constraintBottom_toBottomOf="parent"        card_view:layout_constraintEnd_toStartOf="@+id/mail"        card_view:layout_constraintTop_toTopOf="parent">        <ImageView            android:layout_width="26dp"            android:layout_height="20dp"            android:layout_gravity="center"            android:layout_margin="3dp"            android:layout_marginTop="0dp"            android:layout_marginRight="10dp" />    </androidx.cardview.widget.CardView>    <androidx.cardview.widget.CardView        android:id="@+id/mail"        android:layout_width="45dp"        android:layout_height="30dp"        android:clickable="true"        android:foreground="?android:attr/selectableItemBackground"        android:orientation="vertical"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/call"        card_view:cardBackgroundColor="@color/colorPrimaryDark"        card_view:cardCornerRadius="20dp"        card_view:cardElevation="7dp"        card_view:layout_constraintBottom_toBottomOf="parent"        card_view:layout_constraintEnd_toEndOf="parent"        card_view:layout_constraintTop_toTopOf="parent">        <ImageView            android:layout_width="20dp"            android:layout_height="20dp"            android:layout_gravity="center"            android:layout_margin="3dp"            android:layout_marginTop="0dp"            android:layout_marginRight="10dp" />    </androidx.cardview.widget.CardView></androidx.constraintlayout.widget.ConstraintLayout>