<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorPrimary"    tools:context=".Activity.Splash">    <ImageView        android:id="@+id/imageView7"        android:layout_width="200dp"        android:layout_height="200dp"        android:layout_marginTop="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/ic_jnulogo" />    <TextView        android:id="@+id/textView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="32dp"        android:fontFamily="@font/hind_siliguri_bold"        android:text="JnU Diary 2020-21"        android:textColor="#FEFDFD"        android:textColorHint="#FFFFFF"        android:textSize="30sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imageView7" /></androidx.constraintlayout.widget.ConstraintLayout>